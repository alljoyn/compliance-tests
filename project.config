[project]
	description = Compliance tests
[receive]
	requireContributorAgreement = false
	requireSignedOffBy = true
	requireChangeId = true
[submit]
	mergeContent = true
[capability]
	administrateServer = group Administrators
	priority = batch group Non-Interactive Users
	streamEvents = group Non-Interactive Users
	streamEvents = group Stream Events
	accessDatabase = group Database Admins
[access "refs/*"]
	read = group Administrators
	read = group Anonymous Users
	read = group Non-Interactive Users
	read = group Registered Users
	deleteDrafts = group Administrators
[access "refs/for/refs/*"]
	push = group Registered Users
[access "refs/heads/*"]
	create = group Administrators
	create = group ldap/gerrit_compliance_committers
	forgeAuthor = group ldap/gerrit_compliance_committers
	forgeCommitter = group ldap/gerrit_compliance_committers
	push = +force group Administrators
	push = group ldap/gerrit_compliance_committers
	label-Code-Review = -2..+2 group ldap/gerrit_compliance_committers
	label-Code-Review = -1..+1 group Registered Users
	submit = group ldap/gerrit_compliance_committers
	editTopicName = group ldap/gerrit_compliance_committers
[access "refs/meta/config"]
	read = group Administrators
	read = group ldap/gerrit_compliance_committers
	push = group Administrators
[access "refs/tags/*"]
	pushTag = +force group Administrators
	pushTag = group ldap/gerrit_compliance_committers
	pushSignedTag = group ldap/gerrit_compliance_committers
	push = +force group Administrators
[label "Code-Review"]
	function = MaxWithBlock
	copyMinScore = true
	value = -2 Do not submit
	value = -1 I would prefer that you didn't submit this
	value =  0 No score
	value = +1 Looks good to me, but someone else must approve
	value = +2 Looks good to me, approved
[access "refs/notes/commits"]
	push = group Administrators
	create = group Administrators
	requireSignedOffBy = false
	requireChangeId = false
[access "refs/for/*"]
	pushMerge = group Administrators
	pushMerge = group ldap/gerrit_compliance_committers
